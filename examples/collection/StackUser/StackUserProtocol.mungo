package CoCoPOPS.demos.collection.StackUser;

import CoCoPOPS.demos.collection.Stack;

typestate StackUserProtocol {
	Init = {
		Stack produce(Stack, int) : Consume,
		Stack produce(Stack) : Consume,
		void close() : end
	}

	Consume = {
		Stack produce(Stack, int) : Consume,
		Stack produce(Stack) : Consume,
		Stack consume(Stack) : Init
	}
}
